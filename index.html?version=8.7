      document.addEventListener("DOMContentLoaded", function initDownloadNextStepsPage() {
  function getQueryParameter(variable) {
    var query = window.location.search.substring(1);
    var vars = query.split("&");
    for (var i = 0; i < vars.length; i++) {
      var pair = vars[i].split("=");
      if (decodeURIComponent(pair[0]) === variable) {
        return decodeURIComponent(pair[1]);
      }
    }
    return null;
  }

  // Retrieve distribution version and type from the URL
  function computeDownloadUrl() {
    var distributionVersion = getQueryParameter("version");
    var distributionFormat = getQueryParameter("format");
    if (distributionVersion !== null && distributionFormat !== null) {
      return "https://services.gradle.org/distributions/gradle-" + distributionVersion + "-" + distributionFormat + ".zip";
    }
    return null;
  }

  function setDirectHref(href) {
    document.getElementById("direct-link").setAttribute("href", href);
  }

  function setChecksumHref(href) {
    document.getElementById("checksum-link").setAttribute("href", href);
  }

  function triggerDownload(url) {
    window.location = url;
  }

  var downloadUrl = computeDownloadUrl();
  if (downloadUrl !== null) {
    setDirectHref(downloadUrl);
    setChecksumHref(downloadUrl + ".sha256");
    triggerDownload(downloadUrl);
  }
});

